{
  "name": "Being Social",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This is used to track  the amount of time spent on social websites.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"tabs"
  ],
  "background": {
   "scripts": ["background.js"]
  }
}